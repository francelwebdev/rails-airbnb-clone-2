<div class="container">
	<div class="row">
		<div class="col-xs-12 col-sm-6 col-sm-offset-3">
			<h1>Your rental bookings</h1>
				<% @rent_bookings.each do |booking| %>
				<div>
					<h3><%= booking.treehouse.name %></h3>
					<p><strong>Check In:</strong> <%= booking.start_date %></p>
					<p><strong>Check Out:</strong> <%= booking.end_date %></p>
					<p><%= link_to "Cancel booking", booking_path(booking), method: :delete %></p>
				</div>
				<% end %>

			
		</div>
	</div>
</div>
